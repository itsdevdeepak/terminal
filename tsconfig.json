{
  "compilerOptions": {
    "outDir": "dist",
    "module": "ES2020",
    "target": "ES2020",
    "noFallthroughCasesInSwitch": true,
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "moduleResolution": "node"
  },
  "exclude": ["node_modules"]
}
